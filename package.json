{
  "name": "game-planner",
  "version": "1.0.0",
  "description": "A Node and React app to plan game travel",
  "main": "server/index.js",
  "scripts": {
    "serve": "node public/server",
    "start": "run-s clean build-server build-client serve",
    "dev": "run-p dev-server dev-client",
    "dev-server": "npm run build-server && nodemon public/server",
    "dev-client": "parcel watch --no-cache app/views/**/*.pug -d ./public",
    "build": "run-s build-server build-client",
    "build-server": "babel --no-babelrc server/*.js -d public/server --plugins=@babel/plugin-transform-async-to-generator --presets=@babel/preset-env",
    "build-client": "parcel build --no-cache app/views/**/*.pug -d public",
    "clean": "rimraf public/*",
    "webpack": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psullivan6/game-planner.git"
  },
  "author": "Patrick Sullivan <psullivan6@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psullivan6/game-planner/issues"
  },
  "homepage": "https://github.com/psullivan6/game-planner#readme",
  "dependencies": {
    "@date-io/date-fns": "^1.3.9",
    "@material-ui/core": "^4.3.3",
    "@material-ui/pickers": "^3.2.3",
    "axios": "^0.19.0",
    "contentful": "^7.9.1",
    "core-js": "^3.2.1",
    "date-fns": "^2.0.0-beta.3",
    "dotenv": "^8.1.0",
    "http-errors": "^1.7.3",
    "mapbox-gl": "^1.2.1",
    "pug": "^2.0.4",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-async-to-generator": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.2.2",
    "eslint-plugin-react-hooks": "^2.0.1",
    "express": "^4.17.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "react-scripts": "^3.1.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-merge": "^4.2.1"
  }
}
